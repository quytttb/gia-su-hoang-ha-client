    "firebase": "^11.8.1",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^12.18.1",
    "lucide-react": "^0.511.0",
    "react": "^18.2.0",
    "react-calendar": "^5.1.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.4.2",
    "react-helmet-async": "^2.0.5",
