{
  "name": "gia-su-hoang-ha-client",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:vercel": "npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:seo": "./scripts/test-seo.sh",
    "test:seo:prod": "./scripts/test-seo.sh https://giasuhoangha.com",
    "migrate:mock-data": "node scripts/migrate-mock-data.cjs",
    "migrate:tutors": "node scripts/migrate-tutors.cjs",
    "init:center-info": "node scripts/init-center-info.cjs",
    "add:schedules": "node scripts/add-schedules.cjs",
    "create:courses": "node scripts/create-courses.cjs",
    "deploy": "./scripts/deploy.sh",
    "deploy:prod": "./scripts/deploy.sh --production",
    "deploy:quick": "./scripts/deploy-quick.sh",
    "deploy:quick:prod": "./scripts/deploy-quick.sh --production"
  },
